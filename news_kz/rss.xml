&lt;div class=&quot;field field-type-link field-field-source&quot;&gt;
  &lt;div class=&quot;field-items&quot;&gt;
        &lt;div class=&quot;field-item even&quot;&gt;
          &lt;table width=100%&gt;&lt;tr&gt;&lt;td align=left&gt;25.02.2011&amp;nbsp;в&amp;nbsp;11:51&lt;/td&gt;&lt;td align=right&gt;Источник:&amp;nbsp;&lt;a href=&quot;http://vesti.kz/society/78552/&quot; target=&quot;_blank&quot; rel=&quot;nofollow&quot;&gt;vesti.kz&lt;/a&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;        &lt;/div&gt;
